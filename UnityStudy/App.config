<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="raiSection" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>

  <raiSection>
    <alias alias="TarClass" type="UnityStudy.AOPTarget, UnityStudy" />
    <alias alias="CncBehavior" type="UnityStudy.MyInterception, UnityStudy" />
    <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension, Microsoft.Practices.Unity.Interception.Configuration" />
    <container name="hakuContainer">
      <extension type="Interception" />
      <register type="TarClass">
        <interceptor type="TransparentProxyInterceptor" />
        <interceptionBehavior type="CncBehavior" />
        <policyInjection />
      </register>
    </container>
  </raiSection>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

</configuration>